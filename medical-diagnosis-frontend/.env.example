# ==============================================
# MEDICAL DIAGNOSIS PLATFORM - ENVIRONMENT CONFIG
# ==============================================
# Copy this file to .env and fill in your actual API keys
# Never commit .env to version control
# ==============================================

# ============================================
# WHO ICD-API Configuration
# ============================================
# Register at: https://icd.who.int/icdapi
# Required for: ICD-10/ICD-11 diagnosis coding
VITE_WHO_ICD_CLIENT_ID=your_client_id_here
VITE_WHO_ICD_CLIENT_SECRET=your_client_secret_here
VITE_WHO_ICD_TOKEN_ENDPOINT=https://icdaccessmanagement.who.int/connect/token
VITE_WHO_ICD_API_BASE_URL=https://id.who.int

# ============================================
# NCBI PubMed E-utilities
# ============================================
# Register at: https://www.ncbi.nlm.nih.gov/account/
# Required for: Medical literature search
# Optional but recommended for higher rate limits
VITE_PUBMED_API_KEY=your_pubmed_api_key_here
VITE_PUBMED_EMAIL=your_email@example.com

# ============================================
# DrugBank Clinical API
# ============================================
# Register at: https://go.drugbank.com/
# Required for: Drug information and interaction checking
VITE_DRUGBANK_API_KEY=your_drugbank_api_key_here

# ============================================
# AI Provider APIs
# ============================================

# OpenAI (GPT-4, GPT-3.5)
# Register at: https://platform.openai.com/
VITE_OPENAI_API_KEY=sk-your_openai_key_here
VITE_OPENAI_ORG_ID=org-your_org_id_here

# Anthropic (Claude 3)
# Register at: https://console.anthropic.com/
VITE_ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here

# Google AI (Gemini)
# Register at: https://makersuite.google.com/
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Local LLM (Ollama - Optional)
# Download: https://ollama.ai/
# No API key required - runs locally
VITE_OLLAMA_BASE_URL=http://localhost:11434
VITE_OLLAMA_MODEL=llama2

# ============================================
# Infermedica Symptom Checker (Optional)
# ============================================
# Register at: https://developer.infermedica.com/
# Enhanced symptom analysis and triage
VITE_INFERMEDICA_API_KEY=your_infermedica_key_here
VITE_INFERMEDICA_APP_ID=your_app_id_here

# ============================================
# Supabase Backend (Recommended for Production)
# ============================================
# Sign up at: https://supabase.com/
# Required for: Database, Auth, Storage
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# ============================================
# ClinicalTrials.gov API
# ============================================
# No API key required - public API
VITE_CLINICALTRIALS_BASE_URL=https://clinicaltrials.gov/api/v2

# ============================================
# Application Configuration
# ============================================
VITE_APP_NAME=Medical Diagnosis Platform
VITE_APP_VERSION=1.0.0

# Usage Mode: clinical_setting | clinical_study | student | full_hospital | self_exploration
VITE_USAGE_MODE=clinical_setting

# Enable/Disable Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AI_DIAGNOSIS=true
VITE_ENABLE_FHIR=true
VITE_HIPAA_MODE=true

# ============================================
# Security & Compliance
# ============================================
# Session timeout in minutes
VITE_SESSION_TIMEOUT=30

# Maximum failed login attempts
VITE_MAX_LOGIN_ATTEMPTS=5

# Enable audit logging
VITE_AUDIT_LOGGING=true

# Data encryption at rest
VITE_ENCRYPTION_ENABLED=true

# ============================================
# Monitoring & Analytics (Optional)
# ============================================
# Sentry for error tracking
VITE_SENTRY_DSN=your_sentry_dsn_here

# Google Analytics
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Posthog for product analytics
VITE_POSTHOG_API_KEY=your_posthog_key_here
VITE_POSTHOG_HOST=https://app.posthog.com

# ============================================
# Development vs Production
# ============================================
# Set to 'development' or 'production'
NODE_ENV=production

# Enable debug logging (only in development)
VITE_DEBUG_MODE=false

# ============================================
# Rate Limiting
# ============================================
# API request rate limits (requests per minute)
VITE_PUBMED_RATE_LIMIT=10
VITE_DRUGBANK_RATE_LIMIT=60
VITE_OPENAI_RATE_LIMIT=50

# ============================================
# FHIR Configuration
# ============================================
VITE_FHIR_VERSION=4.0.1
VITE_FHIR_BASE_URL=https://your-fhir-server.com/fhir

# ============================================
# Email Service (Optional)
# ============================================
# For sending notifications, reports, etc.
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@gmail.com
VITE_SMTP_PASSWORD=your_email_password

# ============================================
# Backup & Recovery
# ============================================
# S3-compatible storage for backups
VITE_BACKUP_ENABLED=true
VITE_S3_BUCKET=medical-platform-backups
VITE_S3_REGION=us-east-1
VITE_S3_ACCESS_KEY=your_access_key
VITE_S3_SECRET_KEY=your_secret_key

# ============================================
# Notes
# ============================================
# - Never commit this file with real API keys
# - Add .env to .gitignore
# - Use different .env files for development (.env.development) and production (.env.production)
# - Rotate API keys regularly for security
# - For production, use environment variable management tools (AWS Secrets Manager, HashiCorp Vault, etc.)
# - Ensure all third-party services have signed BAAs for HIPAA compliance
